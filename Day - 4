lower = 1042000
upper = 702648265

for num in range(lower, upper+1):
    # find the sum of the cube of each digit
    temp = num
    # initialize sum
    sum = 0
    order = len(str(num))
    while temp > 0:
        digit = temp % 10
        sum += digit ** order
        temp //= 10

# display the result
    if num == sum:
        print(num,"is the First Armstrong number")
        break

